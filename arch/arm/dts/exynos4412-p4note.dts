/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include "exynos4412.dtsi"

/ {
	model = "Samsung p4note boards based on Exynos4412";
	compatible = "samsung,p4note", "samsung,exynos4412", "samsung,exynos4";

	aliases {
		mmc0 = &mshc_0;
		mmc2 = &sdhci2;
	};

	chosen {
		stdout-path = &serial_2;
	};

	pwm: pwm@139d0000 {
		compatible = "samsung,exynos4210-pwm";
		reg = <0x139d0000 0x1000>;
		samsung,pwm-outputs = <0>, <1>, <2>, <3>;
		#pwm-cells = <3>;
	};

	backlight: backlight {
		compatible = "pwm-backlight";
		pwms = <&pwm 1 32768 0>;
		brightness-levels = <0 100 500 1000 1500 2000 2500 2800>;
		default-brightness-level = <6>;
		enable-gpios = <&gpm0 1 GPIO_ACTIVE_HIGH>;
		power-supply = <&ldo23_reg>;
	};

	panel_vdd: voltage-regulator-1 {
		compatible = "regulator-fixed";
		regulator-name = "LCD_ENABLE";
		gpio = <&gpc0 1 GPIO_ACTIVE_HIGH>;
		enable-active-high;
		regulator-boot-on;
	};

	panel: panel {
		compatible = "simple-panel";
		power-supply = <&panel_vdd>;
		backlight = <&backlight>;
		enable-gpios = <&gpm0 5 GPIO_ACTIVE_HIGH>;
	};

	fimd@11c00000 {
		u-boot,dm-pre-reloc;
		compatible = "samsung,exynos-fimd";
		reg = <0x11c00000 0xa4>;

		samsung,vl-freq = <60>;
		samsung,vl-col = <1280>;
		samsung,vl-row = <800>;
		samsung,vl-width = <217>;
		samsung,vl-height = <135>;

		samsung,vl-clkp;
		samsung,vl-hsp;
		samsung,vl-vsp;
		samsung,vl-dp;

		samsung,vl-bpix = <4>;

		samsung,vl-hspw = <16>;
		samsung,vl-hbpd = <36>;
		samsung,vl-hfpd = <18>;
		samsung,vl-vspw = <3>;
		samsung,vl-vbpd = <16>;
		samsung,vl-vfpd = <4>;
		samsung,vl-cmd-allow-len = <0xf>;

		samsung,power-on-delay = <30>;
		samsung,winid = <3>;
		samsung,interface-mode = <1>;
		samsung,dp-enabled = <0>;

		samsung,logo-on = <0>;
		samsung,rgb-mode = <0>;
	};
};

&i2c_7 {
	samsung,i2c-sda-delay = <100>;
	samsung,i2c-slave-addr = <0x10>;
	samsung,i2c-max-bus-freq = <400000>;
	status = "okay";

	max77686: max77686_pmic@9 {
		compatible = "maxim,max77686";
		interrupt-parent = <&gpx0>;
		interrupts = <7 0>;
		reg = <0x09>;
		#clock-cells = <1>;

		voltage-regulators {
			ldo1_reg: LDO1 {
        regulator-compatible = "LDO1";
				regulator-name = "ldo1";
				regulator-always-on;
				regulator-boot-on;
			};

			ldo2_reg: LDO2 {
        regulator-compatible = "LDO2";
				regulator-name = "ldo2";
				regulator-always-on;
				regulator-boot-on;
			};

			ldo3_reg: LDO3 {
        regulator-compatible = "LDO3";
				regulator-name = "VCC_1.8V_AP";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;
			};

			ldo4_reg: LDO4 {
        regulator-compatible = "LDO4";
				regulator-name = "ldo4";
				regulator-always-on;
				regulator-boot-on;
			};

			ldo5_reg: LDO5 {
        regulator-compatible = "LDO5";
				regulator-name = "VCC_1.8V_IO";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;

				regulator-state-mem {
					regulator-off-in-suspend;
				};
			};

			ldo6_reg: LDO6 {
        regulator-compatible = "LDO6";
				regulator-name = "ldo6";
				regulator-always-on;
				regulator-boot-on;
			};

			ldo7_reg: LDO7 {
        regulator-compatible = "LDO7";
				regulator-name = "ldo7";
				regulator-always-on;
				regulator-boot-on;
			};

			ldo8_reg: LDO8 {
        regulator-compatible = "LDO8";
				regulator-name = "VMIPI_1.0V";
				regulator-min-microvolt = <1000000>;
				regulator-max-microvolt = <1000000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
				};
			};

			ldo9_reg: LDO9 {
        regulator-compatible = "LDO9";
				regulator-name = "VLED_IC_1.9V";
				regulator-min-microvolt = <1950000>;
				regulator-max-microvolt = <1950000>;
        regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-off-in-suspend;
				};
			};

			ldo10_reg: LDO10 {
        regulator-compatible = "LDO10";
				regulator-name = "VMIPI_1.8V";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-on-in-suspend;
				};
			};

			ldo11_reg: LDO11 {
        regulator-compatible = "LDO11";
				regulator-name = "VABB1_1.9V";
				regulator-min-microvolt = <1950000>;
				regulator-max-microvolt = <1950000>;
				regulator-always-on;
				regulator-boot-on;

				regulator-state-mem {
					regulator-off-in-suspend;
				};
			};

			ldo12_reg: LDO12 {
        regulator-compatible = "LDO12";
				regulator-name = "VUOTG_3.0V";
				regulator-min-microvolt = <3000000>;
				regulator-max-microvolt = <3000000>;
